<Window x:Class="MasterCrewExplorer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MasterCrewExplorer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="ProcessFile" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="682,51,0,0" Click="Button_Click" Height="30"/>
        <TextBlock HorizontalAlignment="Left" x:Name="tempTextBlock" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Margin="10,241,0,0" Height="168" Width="647"/>
        <TextBox HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" x:Name="FileLocationTextBox" Text="FileLocation" VerticalAlignment="Top" Width="678" IsReadOnly="True" Margin="10,10,0,0"/>
        <Button Content="Load File" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="707,13,0,0" Click="LoadFileButton_Click"/>
        <CheckBox Content="Use Repository" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="578,59,0,0" Checked="CheckBox_Checked"/>

    </Grid>
</Window>
